{
    "manifest_version": 3,
    "name": "Birthday Calendar Extension",
    "version": "1.0",
    "description": "Adds birthdays to Google Calendar",
    "permissions": [
      "identity",
      "https://www.googleapis.com/*",
      "storage"
    ],
    "options_page": "index.html",
    "browser_action": {
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "icons": {
      "19": "icon19.png",
      "32": "icon32.png",
      "38": "icon38.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },  
    "oauth2": {
        "client_id": "795290528330-a2ftr4mqv6lcjokpubjej4ccjajfrcpo.apps.googleusercontent.com",
        "scopes": [
          "https://www.googleapis.com/auth/calendar.events"
        ]
      }
  }